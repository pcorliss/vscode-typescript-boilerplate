{
  "name": "vscode-typescript-boilerplate",
  "version": "0.0.0",
  "description": "Typescript Boilerplate with VS Code Support",
  "engines": {
    "node": ">= 14.16 <15"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.35",
    "chai": "^4.3.4",
    "mocha": "^8.3.2",
    "rimraf": "^3.0.2",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "start": "node build/src/main.js",
    "clean": "rimraf build tmp",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "test": "mocha -r ts-node/register tests/**/*.spec.ts",
    "test:watch": "mocha -r ts-node/register -w --watch-files 'src/**/*.ts,tests/**/*.spec.ts' tests/**/*.spec.ts"
  },
  "author": "Philip Corliss <pcorliss@gmail.com>",
  "license": "MIT"
}
